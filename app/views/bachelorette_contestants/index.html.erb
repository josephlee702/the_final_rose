<h2><u><%= @bachelorette.name %></u></h2>

<h3><u>Bachelorettes</u>:</h3>
  <% @bachelorette.contestants.each do |contestant| %>
    <%= form_with(url: "/contestants/#{contestant.id}", method: :get, local: true) do |form| %>
      <%= link_to "#{contestant.name}", "/contestants/#{contestant.id}?bachelorette_id=#{@bachelorette.id}" %>
    <% end %>
    <%= "Age: #{contestant.age}" %><br>
    <%= "Hometown: #{contestant.hometown}" %>
  <% end %>